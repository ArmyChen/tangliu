@charset "utf-8";
/* CSS Document */
/*初始化样式*/
html { -ms-touch-action: none;font-size: 12px;-webkit-text-size-adjust: 100%;  }
body,div,dl,dt,dd,ol,ul,li,img,a,form,input,label,p,span,hr,h1,h2,h3,h4,h5,h6,select,option,textarea{ padding:0; margin:0;  }
.clearfix:after { content: ".";display: block;height: 0;	clear: both;visibility: hidden;	font-size: 0; }
.clearfix {}
.f_l,.fL{ float:left;}
.f_r,.fR{ float:right;}
img{  border: 0px solid; }
ul,ol,dl,li,dd,dt{ list-style:none; }
a img{ border:0; }
img,input,label{ border:0; font-size: 1.1rem;}
a { text-decoration:none; color:inherit; }
a,button,input{-webkit-tap-highlight-color:rgba(255,0,0,0);}
body{font-size:1.1rem; font-family: Tahoma, Arial,"Droid Sans","Helvetica Neue","Droid Sans Fallback", "Heiti SC",Microsoft Yahei, Roboto, sans-self; background:#f5f5f5; width:32rem; margin:0 auto;  max-width:32rem;}
/*去除手机表单的默认样式*/
input,select,textarea { border:none; border-radius:0; box-shadow:none;/*不能拖拽改变大小*/ resize:none;}
input,select,textarea {
	outline:none;
	box-shadow:inset 0 0px 0px rgba(0,0,0,0);
	font-family:inherit;
}
input {}

input[type="button"],input[type="submit"],input[type="reset"]{-webkit-appearance: none;}
input[type=checkbox]{
        -webkit-appearance: none;
        appearance: none;
        width: 20px;
        height:20px;
        margin: 0;
        cursor: pointer;
        vertical-align: bottom;
        background: #fff;
        border:1px solid  #dddddd;
  
    
        -webkit-box-sizing: border-box;
        -moz-box-sizing: border-box;
        box-sizing: border-box;
        position: relative;
}

input[type=checkbox]:active:active {
        border:1px solid  #dddddd;
        background: #ebebeb;
}       
        
input[type=checkbox]:hover {
        border-color: #c6c6c6;
       
}

input[type=checkbox]:checked {
        background: #fff;
}

input[type=checkbox]:checked::after {
        content: url(../images/gou.jpg); 
		width: 20px;
        height: 20px;
        display: block;
        position: absolute;
        top: 0px;
        right: 0px;
        left: 0px
}

input[type=checkbox]:focus {
        outline: none;
         border:1px solid  #dddddd;
}













input[type=radio]{
-webkit-appearance: none;
border:1px solid  #dddddd;
 position: absolute;
 width: 20px !important;
        height: 20px;
        display: inline-block;
        position:relative ;
        top: 0px;
        right: 0px;
        left: 0px
		vertical-align:middle;
background:url(../images/gous.jpg) no-repeat; 
}

input[type=radio]:checked::after {
        content: url(../images/gou.jpg); 
		width: 20px;
        height: 20px;
        display: block;
        position: absolute;
        top: 0px;
        right: 0px;
        left: 0px
}

input[type=radio]:focus {
        outline: none;
         border:1px solid  #dddddd;
}






/*公共样式*/
/*定位*/
.fL { float:left;}
.fR { float:right;}
.right{ right:0;}
.left { left:0;}
/*显示*/
.hide{display: none;}
/*字体颜色*/
.c666 { color:#666;}
.cbe { color:#bebebe;}

/*头部*/
.header { position:relative; background:#be0000 none repeat scroll 0 0}
.header h2 { position:absolute; top:0; width:100%; line-height:4.4rem; text-align:center; font-weight:normal; color:white; letter-spacing:0.3rem;  background:#be0000;height:4rem;  }
.header a  { position:absolute; display:block; width:15%; height:100%; top:0; background:url(../images/historyback.png) no-repeat 42% center; background-size:20%; }
.header .right { background:url(../images/homepage.png) no-repeat 28% center; background-size:50%;}


/*轮播样式*/

.swipe {
	overflow: hidden;
	visibility: hidden;
	position: relative;
}
.swipe-wrap {
	overflow: hidden;
	position: relative;
}
.swipe-wrap li{
	float: left;
	width: 100%;
	position: relative;
}
.swipe #position { width:32rem; position:absolute; bottom:0.3rem; text-align:center;}
.swipe #position ol { display:inline-block; padding:0.3rem;bottom:1rem; background:url(../../img/mob/indexbannerbg.png); border-radius:1.5rem; -moz-border-radius:1.5rem;}
.swipe #position li {float:left; margin:0 0.5rem; padding:0.5rem; border:0.2rem white solid; border-radius:0.7rem; -moz-border-radius:0.7rem;}
.swipe #position .on{ background:white;}
.swipe-wrap li img{width:100%;}
/*轮播样式*/


/*顶部导航*/
.index .topNav { height:5.4rem; background:url(../../img/mob/index_navtop.png) no-repeat 0 0; background-size:contain; border-bottom:0.1rem #e6e6e6 solid;}
