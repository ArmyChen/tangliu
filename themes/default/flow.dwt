<script language="javascript">
function switch_index(index){
	for(var i = 1; i < 4; i ++){
		document.getElementById('box_'+i).className = '';
		document.getElementById('newhuangou_'+i).style.display = 'none';
	}
	
	document.getElementById('box_'+index).className = 'current';
	document.getElementById('newhuangou_'+index).style.display = '';
}


function checkamount1(id){
		    var obj = document.getElementById('goods_number_'+id);
			var num = obj.value;
			
			var old_num = num;
			try{ 
				num = num*1 -1;
				if (num == 0) num = 1;
			}catch(e){
				num = old_num;
			}
			obj.value = num;
			formCart.submit();
		}
		
function checkamount2(id){
			var obj = document.getElementById('goods_number_'+id);
			var num = obj.value;
			var old_num = num;
			try{ 
				num = num*1 + 1;
			}catch(e){
				num = old_num;
			}
			obj.value = num;
			formCart.submit();
		}



</script>

